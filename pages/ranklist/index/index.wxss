/* pages/ranklist/index/index.wxss */
.container {
  left: 0;
  right: 0;
  top: 0;
  padding-bottom: 0;
  width: 750rpx;
  position: absolute;
  color: white;
}

.tabPanel{
  padding-top: 66rpx
}

.gradientView {
  height: 1000rpx;
}

.navbar {
  z-index: 100;
  width: 100%;
  overflow: hidden;
  position: fixed;
  top: 68rpx;
  left: 0rpx;
  display: flex;
  padding: 20rpx 30rpx;
  justify-content: space-between;
  box-sizing: border-box;
  text-align: center;
  color: rgba(255,255,255,0.5);
}

.navbar .navbar-title {
  display: flex;
  font-size: 22rpx;
}

.navbar .navbar-title .navbar-item {
  text-align: center;
  margin-right: 20rpx;
  padding: 0 20rpx;
  height: 40rpx;
  line-height: 40rpx;
  border: 1rpx solid rgba(255, 255, 255, 0.1);
  border-radius: 21rpx;
}

.selected{
  background: rgba(255, 255, 255, 0.1);
  border-radius: 21rpx;
}

.navbar .navbar-item image {
  width: 10rpx;
  height: 5rpx;
  vertical-align: middle;
  margin-top: -2rpx;
}

.first {
  display: flex;
  margin: 2rpx 0;
  position: relative;
  height: 240rpx;
  flex-direction: column;
  align-items: center;
}

.first image {
  height: 180rpx;
  width: 180rpx;
  border-radius: 50%;
  border: 1rpx solid #F7C20B;
  position: absolute;
}

.first .bg1 {
  position: relative;
  margin-top: 144rpx;
  color: #F7C20B;
  background: white;
  height: 46rpx;
  border: 1rpx solid #F7C20B;
  border-radius: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0rpx 30rpx;
  z-index: 2;
}

.teamname {
  height: 50rpx;
  line-height: 50rpx;
  width: 345rpx;
  overflow: hidden; /*超出一行文字自动隐藏*/
  text-overflow:ellipsis;/*文字隐藏后添加省略号*/
  white-space:nowrap; /*强制不换行*/
  text-align: center;
  font-weight: bold;
}

.first view .tip, .second view .tip {
  text-align: center;
  font-size: 42rpx;
  display: flex;
  height: 42rpx;
  line-height: 42rpx;
  font-weight: bolder;
}

/* .first view .tip .distance,.second view .tip .distance{
  padding-top: 16rpx;
} */

.first view .tip .km, .second view .tip .km {
  font-size: 24rpx;
  margin-top: 16rpx;
  line-height: 24rpx;
}

.next {
  display: flex;
  margin: 2rpx 0;
  position: relative;
  height: 204rpx;
  align-items: center;
  justify-content: space-around;
}

.second {
  display: flex;
  position: relative;
  height: 204rpx;
  flex-direction: column;
  align-items: center;
}

.second image {
  height: 160rpx;
  width: 160rpx;
  border-radius: 50%;
  border: 1rpx solid #F7C20B;
  position: absolute;
  z-index: 1
}

.second .bg1 {
  position: relative;
  color: #F7C20B;
  background: white;
  height: 46rpx;
  margin-top: 124rpx;
  border: 1rpx solid #F7C20B;
  border-radius: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0rpx 30rpx;
  z-index: 2;
}

.second .bg2 {
  color: #F7C20B;
  background: white;
  height: 46rpx;
  border: 1rpx solid #F7C20B;
  border-radius: 24rpx;
  margin-top: -48rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0rpx 30rpx;
}

.cell {
  background: rgba(248, 248, 248, 0.02);
  height: 190rpx;
  margin-top: 28rpx;
  display: flex;
  align-items: center;
}

.left-title {
  padding-left: 76rpx;
  width: 40rpx;
  font-size: 32rpx;
  /* padding-top: 36rpx; */
}

.cell .head {
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
  margin-left: 70rpx;
  color: #F7C20B;
  border: 1rpx solid #F7C20B;
}

.right-view {
  margin-left: 22rpx;
  font-size: 34rpx;
}

.right-view .tip {
  padding: 6rpx 30rpx;
  height: 34rpx;
  line-height: 34rpx;
  border-radius:23rpx; 
  background: #F7C20B;
  text-align: center;
  margin-top: 26rpx;
  font-size: 34rpx;
  font-weight: bolder;
  display: flex;
}

/* .right-view .tip .distance{
  padding-top: 6rpx;
} */

.right-view .tip .km {
  font-size: 24rpx;
  line-height: 24rpx;
  margin-top: 10rpx;
}

.bottom-tip {
  margin: 64rpx 70rpx 0;
  font-size: 24rpx;
}

.listcontainner {
  margin: 25rpx 30rpx;
  border-radius: 30rpx;
  background: white;
  box-shadow: 0rpx 0rpx 10rpx rgba(0, 0, 0, 0.1);
}

.list {
  margin: 0 30rpx;
  padding: 30rpx 0;
  border-bottom: 2rpx solid #f1f1f1;
  display: flex;
  align-items: center;
  height: 60rpx;
}

.list view.index {
  color: #1e1e1e;
  min-width: 40rpx;
  font-size: 30rpx;
}

.list .logo {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  border: 1rpx solid #F7C20B;
}

.list .name {
  flex: 1;
  margin-left: 20rpx;
  font-size: 30rpx;
  color: #1e1e1e;
  overflow: hidden; /*超出一行文字自动隐藏*/
  text-overflow:ellipsis;/*文字隐藏后添加省略号*/
  white-space:nowrap; /*强制不换行*/
}

.list .runVolume {
  font-size: 30rpx;
  color: #1e1e1e;
  display: flex;
}

.runVolumeKM {
  font-size: 22rpx;
  margin-top: 8rpx;
}

.popview {
  position: fixed;
  width: 164rpx;
  height: 240rpx;
  right: 30rpx;
  top: 140rpx;
  z-index: 101;
}

.popview image{
  position: absolute;
  width: 15rpx;
  height: 8rpx;
  top: 0;
  right: 0;
}

.popview .popcontent {
  position: absolute;
  top: 8rpx;
  left: 0;
  width: 164rpx;
  height: 232rpx;
  background: #F8F8F8;
  border-bottom-left-radius: 10rpx;
  border-top-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}

.popview .popcontent view{
  flex: 1;
  text-align: center;
  height: 77rpx;
  line-height: 77rpx;
  font-size: 26rpx;
}

.empty-content{
  padding:100rpx;
  text-align:center;
  align-items: center;
}

.empty-content image{width:150rpx;padding-bottom:20rpx;height: 192rpx;}