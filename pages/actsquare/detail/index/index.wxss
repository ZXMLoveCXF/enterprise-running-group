.pk-box {
    width: 690rpx;
    margin-left: 30rpx;
    padding-top: 70rpx;
}
.none-box{
    display: flex;
    flex-direction: column;
    justify-content: center;
    text-align: center;
    align-items: center;
    margin-top: 200rpx;
    color: #C5C5C5;
    font-size: 28rpx;
}
.none-box image{
    width: 150rpx;
    height: 192rpx;
    margin-bottom: 20rpx;
}

.hrs {
    width: 690rpx;
    height: 1rpx;
    background: rgba(231, 231, 231, 1);
}

.checkbox {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 34rpx 0;
}

.team-index {
    color: #040000;
    font-size: 28rpx;
    height: 118rpx;
    width: 52rpx;
    line-height: 32rpx;
    font-weight: bold;
}

.tip-img image {
    width: 42rpx;
    height: 42rpx;
}

.checkbox .right-img {
    font-family: 'DIN Engschrift Std';
    font-size: 44rpx;
    color: #040000;
}

.group-img {
    width: 118rpx;
    height: 118rpx;
    border-radius: 20rpx;
    margin-right: 20rpx;
    overflow: hidden;
}

.group-center {
    display: flex;
    flex: 1;
    flex-direction: column;
    justify-content: space-between;
    height: 118rpx;
    margin-right: 70rpx;
    font-size: 32rpx;
}

.group-center>text {
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    color: #1E1E1E;
    line-height:40rpx;

}

.center-b {
    color: rgba(169, 169, 169, 1);
    font-size: 26rpx;
}

.center-b image {
    width: 27rpx;
    height: 24rpx;
    margin-right: 8rpx;
}
.pheight{
    padding-top: 100rpx;
    width: 750rpx;
    padding-bottom: 160rpx;
}
.result-tip{
    font-size: 30rpx;
    display: inline-block;
}