/* pages/actsquare/detail/index/index.wxss */

/* pages/actsquare/publish/preview/preview.wxss */

.bg-bor {
    width: 630rpx;
    padding: 30rpx;
    margin: 20rpx 30rpx;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0rpx 0rpx 10rpx rgba(0, 0, 0, 0.1);
    border-radius: 30rpx;
    position: relative;
}

.bg-bor .title {
    color: #1E1E1E;
    font-weight: bold;
    display: inline-block;
}

.bg-bor .text-content {
    display: block;
    font-size: 28rpx;
    margin-top: 30rpx;
    word-break: break-all;
}

.complete-btn {
    width: 100%;
    height: 118rpx;
    line-height: 118rpx;
    font-size: 34rpx;
    border-radius: 0;
    position: fixed;
    bottom: 0;
    color: #fff;
    z-index: 2;
    font-weight: bold;
    opacity: .94;
}

.hr {
    width: 660rpx;
    height: 1rpx;
    background: rgba(231, 231, 231, 1);
    margin-left: -15rpx;
    margin-top: 26rpx;
}

.prize-list {
    margin-top: 30rpx;
    display: flex;
    flex-direction: column;
}

.prize-list view {
    display: flex;
}

.input-right {
    display: flex;
    flex-direction: column;
    width: 438rpx;
}

.input-bottom {
    width: 582rpx;
    padding: 30rpx 24rpx;
    border-radius: 10rpx;
    margin-bottom: 12rpx;
    line-height: 42rpx;
    background: rgba(248, 248, 248, 1);
    font-size: 28rpx;
}

.input-right view {
    width: 400rpx;
    height: 84rpx;
    line-height: 84rpx;
    padding-left: 38rpx;
    background: rgba(248, 248, 248, 1);
    border-radius: 10rpx;
    margin-bottom: 12rpx;
    font-size: 28rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis
}

.prize-img-box {
    width: 180rpx;
    height: 180rpx;
    background: #F8F8F8;
    border-radius: 20rpx;
    overflow: hidden;
    margin-right: 12rpx;
}

.prize-img {
    width: 180rpx;
}

.prize-img-box view {
    /* display: flex; */
    display: none;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.prize-img-box view image {
    width: 54rpx;
    height: 54rpx;
    margin-bottom: 14rpx;
}

.prize-img-box text {
    color: #A9A9A9;
    font-size: 24rpx;
}

/* .switch-list {
    margin-bottom: 160rpx;
} */

.card {
    display: block;
    margin-top: 38rpx;
}

/*
*指示点样式
*/

.swiper-box {
    /* position: absolute;
    top: -22rpx;
    height: 200rpx; */
    height: 420rpx;
    margin-top: -64rpx;
}

.swiper-box .wx-swiper-dot {
    width: 8rpx;
    display: inline-flex;
    height: 8rpx;
    margin-left: 0rpx;
    justify-content: space-between;
}

.swiper-box .wx-swiper-dot::before {
    content: '';
    flex-grow: 1;
    border-radius: 100%;
}

.swiper-box .wx-swiper-dot-active {
    width: 20rpx;
    height: 8rpx;
    border-radius: 8rpx;
}

.slide-logo {
    width: 168rpx;
    height: 168rpx;
    border-radius: 20rpx;
    box-shadow: 0rpx 0rpx 10rpx rgba(0, 0, 0, 0.1);
    margin-bottom: 16rpx;
    margin-top: 10rpx;
    background: #fff;
}

.swiper-item .name {
    margin-bottom: 26rpx;
    width: 90%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    text-align: center;
}

.swiper-item {
    display: flex;
    flex-direction: column;
    align-items: center
}

.item-bar {
    display: flex;
    width: 100%;
    flex-direction: row;
    justify-content: space-between;
}

.item-bar>view {
    flex: 1;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.item-bar>view text {
    font-size: 26rpx;
}

.item-bar>view .num {
    font-size: 54rpx;
    font-family: 'DIN Engschrift Std';
}

.bg-card {
    width: 690rpx;
    margin-top: 60rpx;
    padding: 30rpx 0;
}

.sign {
    position: absolute;
    top: 109rpx;
    right: 0;
    width: 110rpx;
    height: 44rpx;
    line-height: 44rpx;
    background: #3851D0;
    text-align: center;
    color: #fff;
    font-size: 24rpx;
    border-bottom-left-radius: 20rpx;
    border-top-left-radius: 20rpx;
    z-index: 3;
    display: flex;
    justify-content: center;
    align-items: center;
}

.sign image {
    width: 22rpx;
    height: 22rpx;
    margin-right: 8rpx;
}

