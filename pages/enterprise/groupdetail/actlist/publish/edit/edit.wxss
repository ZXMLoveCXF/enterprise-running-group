/* pages/enterprise/groupdetail/actlist/publish/edit/edit.wxss */

textarea {
  border-top: 1px solid #eee;
  font-size: 30rpx;
  padding: 40rpx;
  width: 670rpx;
}

.addImg {
  display: flex;
  width: 670rpx;
  margin: 0 auto;
  flex-wrap: wrap;
}

.addImg image {
  width: 210rpx;
  height: 210rpx;
}

.addImg .img:nth-child(1), .addImg .img:nth-child(4), .addImg .img:nth-child(7) {
  margin-left: 0;
}

.addImg .img {
  position: relative;
  margin-top: 20rpx;
  margin-left: 20rpx;
}

.addImg .img[data-sign=del] .icon-del {
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  top: 0rpx;
  right: 0rpx;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
}

.save {
  background-color: #4db0f4;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  color: #fff;
  position: fixed;
  bottom: 0;
  width: 100%;
  font-size: 30rpx;
  border-radius: 0
}
