/* pages/enterprise/groupdetail/actlist/detail/index/index.wxss */

page {
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
}

.act-list {
  width: 100%;
  box-sizing: border-box;
  padding: 30rpx 30rpx 0;
}


.act-list .act-img-box {
  width: 100%;
  position: relative;
  border-radius: 30rpx;
  box-shadow: 0rpx 0rpx 10rpx rgba(0, 0, 0, 0.1);
  flex-direction: column;
  /* height: 345rpx; */
}

.act-img-box .cell-image {
  top: 0;
  left: 0;
  width: 100%;
  height: 345rpx;
  border-radius: 30rpx;
}

/* .act-img-box .cell-title {
  top: 260rpx;
  left: 0;
  right: 0;
  font-size: 30rpx;
  background: rgba(0, 0, 0, 0.8);
  height: 85rpx;
  line-height: 85rpx;
  color: white;
  position: absolute;
  border-bottom-left-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
} */

.act-img-box .cell-title {
  font-size: 30rpx;
  background: rgba(0, 0, 0, 0.8);
  color: white;
  position: absolute;
  border-bottom-left-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
  padding: 20rpx;
  bottom: 0;
  left: 0;
  right: 0;
}

.act-img-box .cell-title view{
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
}

.act-img-box text {
  position: absolute;
  width: 134rpx;
  color: white;
  top: 0;
  line-height: 48rpx;
  font-size: 26rpx;
  right: 0;
  text-align: center;
  border-bottom-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
}

.act-img-box .time {
  display: flex;
  margin: 0rpx 30rpx;
  border-bottom: 1rpx solid #e7e7e7;
  align-items: center;
  font-size: 28rpx;
  color: #1e1e1e;
  height: 100rpx;
}

.act-img-box .time .tipimg {
  width:24rpx;height:24rpx;
  margin-right: 20rpx;
}


.act-img-box .time .usr-data {
  margin-left: 15rpx;
  display: flex;
  line-height: 40rpx;
}

.act-img-box .time .usr-data image {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  border: 1rpx solid #e0e0e0;
  margin-right: 15rpx;
}

.act-img-box .location {
  display: flex;
  margin: 0rpx 30rpx;
  border-bottom: 1rpx solid #e7e7e7;
  font-size: 28rpx;
  color: #1e1e1e;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}

.act-img-box .location image {
  width:24rpx;height:24rpx;
  margin-right: 20rpx;
  padding-top: 40rpx;
  transform: translateY(-50%);
}

.act-img-box view .awardbtn{
  width: 630rpx;
  height: 96rpx;
  margin: 0 30rpx;
  text-align: center;
  line-height: 96rpx;
  font-size: 30rpx;
  color: #a9a9a9;
  background: #f8f8f8;
  border-radius: 48rpx;
}

.time image.rightPoint {
  padding-left: 20rpx;
  width: 14rpx;
  height: 30rpx;
}

.act-tag {
  width: 108rpx;
  height: 108rpx;
  position: fixed;
  bottom: 150rpx;
  right: 30rpx;
  align-items: center;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  background: white;
  z-index: 999;
}

.act-tag image {
  width: 46rpx;
  height: 46rpx;
}

.thickLine {
  height: 1rpx;
  background-color: #fff;
  width: 100%;
}

.flex1 {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.lotteryTips {
  color: #d9d9d9;
  font-size: 26rpx;
  padding: 0 30rpx 20rpx 70rpx;
}


.navbar {
  overflow: hidden;
  position: relative;
  z-index: 199;
  width: 690rpx;
  background-color: #fff;
  border-top-right-radius: 30rpx;
  border-top-left-radius: 30rpx;
  border-bottom: 2rpx solid #f4f4f4;
}

.navbar.fixed {
  overflow: hidden;
  position: fixed;
  z-index: 998;
  width: 750rpx;
  background-color: #fff;
  top: 0;
  left: 0rpx;
  border-top-right-radius: 30rpx;
  border-top-left-radius: 30rpx;
  padding-left: 30rpx;
  border-bottom: 2rpx solid #f4f4f4;
}

.navbar .inner {
  display: flex;
  height: 100rpx;
}

.navbar .inner .nav {
  width: 230rpx;
  height: 96rpx;
  text-align: center;
  position: relative;
}

.navbar .inner .nav .underLine {
  height: 5rpx;
  width: 26rpx;
  position: absolute;
  top: 96rpx;
  left: 50%;
  transform: translateX(-50%);
}

.navbar .inner .nav view {
  display: inline-block;
  height: 96rpx;
  line-height: 96rpx;
  color: #c1c1c1;
  text-align: center;
  font-size: 28rpx;
}

.addImg {
  display: flex;
  width: 670rpx;
  margin: 0 auto;
  flex-wrap: wrap;
}

.addImg image {
  width: 210rpx;
  height: 210rpx;
}

/* .addImg .img:nth-child(3n+1) {
  margin-left: 0;
} */

.addImg .img {
  position: relative;
  margin-top: 10rpx;
  margin-left: 10rpx;
}

.addImg .img[data-sign=del] .icon-del {
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  top: 0rpx;
  right: 0rpx;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
}

.content {
  padding-bottom: 40rpx;
}

.content .actDetail {
  box-sizing: border-box;
  padding-left: 30rpx;
  padding-right: 30rpx;
}

/* .content .actDetail contentview {
  box-sizing: border-box;
  position: relative;
  width: 630rpx;
} */

.content .actDetail text {
  position: relative;
  color: #1e1e1e;
}

.coachsepline {
  height: 30rpx;
  border-bottom: 2rpx solid #EEEEEE;
  margin-bottom: 30rpx;
}

.dis-none {
  display: none;
}

.inviteJoin {
  background-color: #fff;
  color: #c1c1c1;
  text-align: center;
  height: 118rpx;
  width: 100%;
  font-size: 30rpx;
  position: fixed;
  bottom: 0;
  border-radius: 0;
  display: flex;
  justify-content: space-between;
  border-top: 1rpx solid #f1f1f1;
  align-items: center;
  z-index: 19950104;
}

.actOn {
  background-color: #81c7f8;
  color: #fff;
  text-align: center;
  height: 118rpx;
  width: 100%;
  font-size: 30rpx;
  position: fixed;
  bottom: 0;
  border-radius: 0;
  line-height: 118rpx;
}

.actOver {
  background-color: rgba(193, 193, 193, 1);
  color: #fff;
  text-align: center;
  height: 118rpx;
  width: 100%;
  font-size: 30rpx;
  position: fixed;
  bottom: 0;
  border-radius: 0;
  line-height: 118rpx;
  z-index: 19950104;
}

.inviteJoin .invite, .inviteJoin .signin {
  width: 300rpx;
  background: white;
  display: flex;
  align-items: center;
  text-align: center;
  line-height: 118rpx;
  justify-content: center;
  font-size: 28rpx;
}

.inviteJoin .invite {
  border-right: 1px solid #f1f1f1;
  height: 118rpx;
}

.inviteJoin .noForward {
  width: 100%;
  color: #fff;
  /* line-height: 100rpx; */
  height: 118rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-weight: bold;
}

.inviteJoin image {
  width: 25rpx;
  height: 25rpx;
}

.inviteJoin text {
  display: block;
  font-size: 26rpx;
}

.inviteJoin button {
  background-color: transparent;
  color: #fff;
  font-size: 30rpx;
  border-radius: 0;
  width: 100%;
  line-height: inherit;
  padding: 0
}
.noForward button{
  min-width: 510rpx;
  padding: 20rpx 0;
}

.inviteJoin button::after {
  border: none;
}

.detailImg image {
  width: 100%;
  padding-top: 20rpx;
}

/*mask*/

.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
}

/*content*/

.drawer_attr_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 1001;
  background: #fff;
}

.drawer_content {
  overflow-y: hidden;
  margin-bottom: 100rpx;
}

.drawer_title {
  padding: 30rpx;
  font: 35rpx "microsoft yahei";
  text-align: center;
  border-radius: 0;
  border-bottom: 1rpx solid #e7e7e7;
}

.drawer_title::after {
  border: none;
}

.dis-none {
  display: none !important;
}

.SIBtn::after {
  border: none;
}

.SIBtn text {
  color: #fff;
  font-size: 30rpx;
}

.btn-color-gray {
  background-color: #ccc !important;
  color: #fff;
}

.btn-color-red {
  background-color: #fb4949 !important;
  color: #fff;
}
